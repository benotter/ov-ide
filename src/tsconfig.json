{
    "compilerOptions": {
        "target": "es2017",
        "module": "es6",
        "moduleResolution": "node",
        "jsx": "react-native",
        "sourceMap": true
    },
    "typeAcquisition": {
        "enable": true,
        "include": [
            "node",
            "react",
            "react-dom",
            "reaxt-redux",
            "redux"
        ]
    },
    "include": [
        "./"
    ],
    "exclude":[
        "build",
        "public",
        "node_modules",
        "build-scripts",
    ]
}