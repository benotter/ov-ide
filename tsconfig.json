{
    "compilerOptions": {
        "target": "es2017",
        "module": "es6",
        "moduleResolution": "node",
        "jsx": "react-native",
        "allowJs": true,
    },
    "typeAcquisition": {
        "enable": true,
        "include": [
            "node",
            "react",
            "react-dom",
            "reaxt-redux",
            "redux"
        ]
    },
    "include": [
        "src",
        "types"
    ],
    "exclude":[
        "build",
        "public",
    ]
}